{

    "manifest_version": 2,
    "name": "Osiris Extended",
    "version": "0.1",
  
    "description": "Fixes several problems with the Osiris schedule site",
    
    "permissions": [
        "storage"
    ],
  
    "content_scripts": [
      {
        "matches": ["*://mborijnland.osiris-student.nl/*"],
        "js": ["main.js"],
        "run_at": "document_end"
      }
    ],
    
    "browser_action": {
        "default_popup": "popup/main.html"
    }
  
  }
